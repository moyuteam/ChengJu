  display: flex;
  align-items: center;
  justify-content: center;
}
.logo{
  overflow:hidden;
  display: block;
  width: 200rpx;
  height: 200rpx;
  border: 5rpx solid white;
  border-radius: 50%;
  box-shadow: 0 6rpx 16rpx rgba(25, 118, 210, 0.3);
}
.input_position{
  background-color: rgb(255, 255, 255);
  height:150rpx;
  margin-left: 100rpx;
  margin-right: 100rpx;
  padding: 18rpx;
  top:10rpx;
  border: 5rpx solid white;
  border-radius: 18rpx;
  box-shadow: 0 6rpx 16rpx rgba(25, 118, 210, 0.3);
  align-items: center;
  justify-content: center;  
}
.input{
  margin-top: 10rpx;
  margin-bottom: 10rpx;
